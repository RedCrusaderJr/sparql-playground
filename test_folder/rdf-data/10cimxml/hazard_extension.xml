      <rdfs:label>Hazard.DangerZone</rdfs:label>
      <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://www.custom.org/Storm">
      <rdfs:subClassOf rdf:resource="http://www.custom.org/Hazard"/>
