    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch - Default geospatial",
            "request": "launch",
            "mainClass": "swiss.sib.sparql.playground.Application",
            "projectName": "sparql-playground",
            "args": "8080 geospatial default false"
        },
        {
            "type": "java",
            "name": "Launch - Default geospatial with inferencing",
            "request": "launch",
            "mainClass": "swiss.sib.sparql.playground.Application",
            "projectName": "sparql-playground",
            "args": "8080 geospatial default true"
        },
        {
            "type": "java",
            "name": "Attach - Default geospatial",
            "request": "attach",
            "projectName": "sparql-playground",
            "hostName": "localhost",
            "port": 8801,
            "processId": "${command:PickJavaProcess}"
        },
        {
            "type": "java",
            "name": "Launch - Native geospatial",
            "request": "launch",
            "mainClass": "swiss.sib.sparql.playground.Application",
            "projectName": "sparql-playground",
            "args": "8080 geospatial native false"
        },
        {
            "type": "java",
            "name": "Launch - Native geospatial with inferencing",
            "request": "launch",
            "mainClass": "swiss.sib.sparql.playground.Application",
            "projectName": "sparql-playground",
            "args": "8080 geospatial native true"
        },
        {
            "type": "java",
            "name": "Attach - Native geospatial",
            "request": "attach",
            "projectName": "sparql-playground",
            "hostName": "localhost",
            "port": 8802,
            "processId": "${command:PickJavaProcess}"
        },
        {
            "type": "java",
            "name": "Launch - Marklogic geospatial",
            "request": "launch",
            "mainClass": "swiss.sib.sparql.playground.Application",
            "projectName": "sparql-playground",
            "args": "8080 geospatial marklogic false java localhost 8111 sparql-playground-db"
        },
        {
            "type": "java",
            "name": "Marklogic geospatial with inferencing",
            "request": "launch",
            "mainClass": "swiss.sib.sparql.playground.Application",
            "projectName": "sparql-playground",
            "args": "8080 geospatial marklogic true java localhost 8111 sparql-playground-db"
        },
        {
            "type": "java",
            "name": "Attach - Marklogic geospatial",
            "request": "attach",
            "projectName": "sparql-playground",
            "hostName": "localhost",
            "port": 8803,
            "processId": "${command:PickJavaProcess}"
        },
    ]
}